generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model workout {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  date   DateTime? @db.Date
  workoutName String?
  workoutType    String
  sets Int?
  reps Int?
  minuteDuration  Int
  moodBefore      Int?
  moodAfter    Int?
  notes       String?
  createdOn   DateTime? @db.Date
  
}
